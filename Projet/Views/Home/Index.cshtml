@{
     Layout = null;
}


<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

    <title>@ViewData["Title"] - Emploi du temps - ENSA Safi</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link rel="shortcut icon" href="~/asset/images/safilogo.png" style="border-radius: 50%; width:200px; height:200px;" />

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">


    <!-- Vendor CSS Files -->
    <link href="~/asset/asset2/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/asset/asset2/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/asset/asset2/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/asset/asset2/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/asset/asset2/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/asset/asset2/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/asset/asset2/css/style.css" rel="stylesheet" asp-append-version="true">


</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top header-transparent" style="background:#0a73f7">
        <div class="container d-flex align-items-center justify-content-between position-relative">

            <div class="logo">
                <h1 class="text-light"><a href="http://www.ensas.uca.ma/"><span>ENSAS</span></a></h1>
               
            </div>

            <nav id="navbar" class="navbar">
                <ul>
                    <li>
                        <a class="nav-link scrollto active" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                   
                        <li>
                        <a class="nav-link scrollto active" id="login" asp-area="Identity" asp-page="/Account/Login">Connexion</a>
                        </li>

                    </ul>
                   

              
                
                <i class="bi bi-list mobile-nav-toggle"></i>
               
            </nav><!-- .navbar -->

        </div>
    </header>
  
    <section id="hero">
        <div class="hero-container" data-aos="fade-up">
            <h1>Bienvenue sur votre plateforme</h1><br>
            <h2>une plateforme qui gère efficacement l'emploi du temps de tous les fillières de ENSA Safi</h2>
            <br />
   
            <a href="#about" class="btn-get-started scrollto"><i class="bx bx-chevrons-down"></i></a>
        </div>
    </section><!-- End Hero -->

    <main id="main">

        <section id="services" class="services">
            <div class="container">
                <div class="section-title" data-aos="fade-in" data-aos-delay="100">
                    <h2>Filières</h2>
                    <p>Découvrez nos différentes filières à l'Ensa Safi, chacune offrant des opportunités d'apprentissage uniques. Notre objectif est de fournir une éducation de qualité supérieure dans divers domaines, tout en mettant en avant nos objectifs et notre engagement envers l'excellence éducative.</p>
                </div>

                <div class="row">
                    <div class="col-md-6 col-lg d-flex align-items-stretch mb-5 mb-lg-0">
                        <div class="icon-box flex-grow-1 d-flex flex-column" data-aos="fade-up">
                            <div class="icon"> <i class="bx bx-brain"></i> </div>
                            <h4 class="title"><a href="">GIIA</a></h4>
                            <p class="description">Génie informatique et intelligence artificielle</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg d-flex align-items-stretch mb-5 mb-lg-0">
                        <div class="icon-box flex-grow-1 d-flex flex-column" data-aos="fade-up" data-aos-delay="100">
                            <div class="icon"><i class="bx bx-network-chart"></i></div>
                            <h4 class="title"><a href="">GTR</a></h4>
                            <p class="description">Génie télécommunications et réseaux</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg d-flex align-items-stretch mb-5 mb-lg-0">
                        <div class="icon-box flex-grow-1 d-flex flex-column" data-aos="fade-up" data-aos-delay="200">
                            <div class="icon"><i class="bi bi-airplane"></i></div>
                            <h4 class="title"><a href="">GATE</a></h4>
                            <p class="description">Génie aéronautique et technologies de l'espace</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg d-flex align-items-stretch mb-5 mb-lg-0">
                        <div class="icon-box flex-grow-1 d-flex flex-column" data-aos="fade-up" data-aos-delay="300">
                            <div class="icon"><i class="bx bx-cog"></i></div>
                            <h4 class="title"><a href="">GINDUS</a></h4>
                            <p class="description">Génie industriel</p>
                        </div>
                    </div>

                    <div class="col-md-6 col-lg d-flex align-items-stretch mb-5 mb-lg-0">
                        <div class="icon-box flex-grow-1 d-flex flex-column" data-aos="fade-up" data-aos-delay="300">
                            <div class="icon"><i class="bx bx-atom"></i></div>
                            <h4 class="title"><a href="">GPMA</a></h4>
                            <p class="description">Génie Procédé et matériaux avancés</p>
                        </div>
                    </div>
                </div>
            </div>
        </section><!-- End Services Section -->
        <section id="counts" class="counts section-bg">
            <div class="container">
                <div class="row no-gutters">
                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch">
                        <div class="count-box card" style="background-color: #0a73f7;">
                            <i class="bi bi-check2-all text-white"></i>
                            <span data-purecounter-start="0" data-purecounter-end="6" data-purecounter-duration="1" class="purecounter text-white"></span>
                            <p class="text-white"><strong>Choix de carrière</strong></p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch">
                        <div class="count-box card" style="background-color: #0a73f7;">
                            <i class="bi bi-people text-white"></i>
                            <span data-purecounter-start="0" data-purecounter-end="977" data-purecounter-duration="1" class="purecounter text-white"></span>
                            <p class="text-white"><strong>Élèves ingénieurs</strong></p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch">
                        <div class="count-box card" style="background-color: #0a73f7;">
                            <i class="bi bi-eyeglasses text-white"></i>
                            <span data-purecounter-start="0" data-purecounter-end="50" data-purecounter-duration="1" class="purecounter text-white"></span>
                            <p class="text-white"><strong>Enseignants chercheurs</strong></p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch">
                        <div class="count-box card" style="background-color: #0a73f7;">
                            <i class="bi bi-trophy text-white"></i>
                            <span data-purecounter-start="0" data-purecounter-end="1566" data-purecounter-duration="1" class="purecounter text-white"></span>
                            <p class="text-white"><strong>Lauréats</strong></p>
                        </div>
                    </div>
                </div>
            </div>
        </section>







        <section id="contact" class="contact section-bg">
            <div class="container" data-aos="fade-up">
                <div class="section-title">
                    <h2>Contactez-nous</h2>
                    <p>Nous sommes joignables de plusieurs manières différentes. Pour entrer en contact avec nous, vous avez les options suivantes :</p>
                </div>
                <div class="row">
                    <div class="col-lg-3 col-md-6">
                        <div class="info-box mb-4 h-100 d-flex flex-column">
                            <i class="bx bx-map"></i>
                            <h3>Notre adresse</h3>
                            <p>Route Sidi Bouzid BP 63 Rue Sidi M'Barek, Safi 46000</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="info-box mb-4 h-100 d-flex flex-column">
                            <i class="bx bx-envelope"></i>
                            <h3>Envoyez-nous un email</h3>
                            <p>ensasafi@gmail.com</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="info-box mb-4 h-100 d-flex flex-column">
                            <i class="bx bx-phone-call"></i>
                            <h3>Appelez-nous</h3>
                            <p>+212 5246-69155</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6">
                        <div class="info-box mb-4 h-100 d-flex flex-column">
                            <i class="bx bxl-facebook"></i>
                            <h3>Nous suivre sur Facebook</h3>
                            <a href="lien-de-votre-page-facebook">Visiter notre page</a>
                        </div>
                    </div>
                </div>
            </div>
        </section>


    </main><!-- End #main -->
    <footer id="footer">
        <div class="footer-top">
            <div class="container">
                <div class="row justify-content-center">
                    <!-- Utilisation de la classe justify-content-center pour centrer horizontalement -->
                    <div class="col-lg-8 col-md-6">
                        <div class="footer-info">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d13485.770343854805!2d-9.2636605!3d32.326826!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0xdac2176b55d2d85%3A0x8f039c8e14c1a1af!2s%C3%89cole%20Nationale%20des%20Sciences%20Appliqu%C3%A9es_Safi!5e0!3m2!1sfr!2sma!4v1691803869495!5m2!1sfr!2sma" frameborder="0" style="border:0; width: 100%; height: 384px;" allowfullscreen></iframe>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="text-center">
                <!-- Utilisation de la classe text-center pour centrer le contenu -->
                <div class="copyright">
                    &copy; Copyright <strong><span>2023-2024</span></strong>. All Rights Reserved
                </div>
                <div class="credits">
                    Developped by Khouy Rajae & Boufardi Ezouhra
                </div>
            </div>
        </div>
    </footer><!-- End Footer -->


    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/asset/asset2/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/asset/asset2/vendor/aos/aos.js"></script>
    <script src="~/asset/asset2/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/asset/asset2/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/asset/asset2/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/asset/asset2/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/asset/asset2/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="~/asset/asset2/js/main.js"></script>

</body>

</html>